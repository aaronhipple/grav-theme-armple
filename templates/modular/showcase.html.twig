{% set source = page.header.source %}
<div class="container">
    <h1>{{ page.title }}</h1>
    {{ content }}
    <div class="frames">
        <div class="frame-wrapper desktop-size">
            <iframe src="{{ source }}" frameborder="0"></iframe>
        </div>
        <div class="frame-wrapper phone-size">
            <iframe src="{{ source }}" frameborder="0"></iframe>
        </div>
    </div>
</div>